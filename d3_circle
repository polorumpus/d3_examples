<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <script type="text/javascript" src="d3.js"></script>
    <script>
    function draw(data) {
        "use strict";


        d3.select("body")
          .append('svg')
            .attr('width', 70)
            .attr('height', 50)
          .selectAll('circle')
          .data(data)
          .enter()
          .append('circle')
            .attr('cx', function(d){return d.x})
            .attr('cy', function(d){return d.y})
            .attr('r', 5)


    }
    </script>
</head>
<body>
    <script>
        d3.json('data/circleexample.json', draw);
    </script>
</body>
</html>
